{
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 8000,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "<UPSTREAM-UUID>",
            "alterId": 0,
            "security": "auto"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/ws"
        }
      },
      "mux": {
        "enabled": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "freedom"
    }
  ],
  "dns": {
    "servers": [
      "8.8.8.8",
      "8.8.4.4",
      "localhost"
    ]
  },
  "log": {
    "loglevel": "warning"
  }
}